{
	"include": [
		"src/**/*.ts"
	],
	"exclude": [
		"src/**/*.test.ts",
		"dist", "out", "node_modules", "webview-ui",
	],
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"outDir": "out",
		"rootDir": "src",
		"sourceMap": true,
		"strict": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"lib": [ "es2022" ]
	}
}
